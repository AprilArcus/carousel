    "react-hot-loader": "^1.2.3",
    "webpack": "^1.7.3",
    "webpack-dev-server": "^1.7.0",
    "browserify": "^9.0.3",
    "babelify": "^5.0.4",
    "uglifyify": "^3.0.1"
  },
  "dependencies": {
    "babel-runtime": "^4.7.8",
    "react": "^0.13.0",
    "flux": "^2.0.1",
